# Application
NODE_ENV=development
PORT=3000
API_PREFIX=/api/v1

# Database (example for PostgreSQL on RDS)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pandebugger
DB_USER=postgres
DB_PASSWORD=your_password_here

# AWS (for deployment)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:5173

# JWT (Authentication)
JWT_SECRET=your_jwt_secret_change_this_in_production_use_long_random_string
JWT_EXPIRES_IN=7d
